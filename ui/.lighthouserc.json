{
  "ci": {
    "collect": {
      "url": [
        "http://localhost:3000/"
      ],
      "startServerCommand": "next build && next start -p 3000",
      "numberOfRuns": 1,
      "startServerReadyPattern": "started server|ready - started server on|http://localhost:3000"
    },
    "assert": {
      "assertions": {
        "categories:pwa": [
          "error",
          {
            "minScore": 0.9
          }
        ]
      }
    },
    "settings": {
      "chromeFlags": "--no-sandbox"
    }
  }
}